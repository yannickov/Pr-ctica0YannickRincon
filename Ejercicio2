// Codigo número 2
#include <iostream>

using namespace std;

int main() 
{
    int i = 1, j = 2;    //Se crean dos variables tipo int
    
    if(i > j && j > i)   // Se usa un condicional con un AND lógico. La condición no se cumple. Nada sucede
        i++;             
    if(i > j || j > i)   //Se usa un condicional con un OR lógico. La condición se cumple y la variable j pasa a tener el valor 3
        j++;
    if(i | j)            // Se usa un condicional OR binario. La condición se cumple y la variable i pasa a tener el valor 2
        i++;
    if(i & j)           //Se usa un condicional AND bianario. La condición se cumple y la variblae j pasa a tener el valor 4
        j++;
    
    cout<<i * j<<endl;   // 2*4 = 8
    return 0;
}
